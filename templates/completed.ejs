<div class="row">
	<div class="col s12">
		<h1>Encuestas completadas</h1>
		<p>A continuación una lista de las encuestas que usted ha completado.</p>
	</div>

	<% surveys.forEach(function(item){ %>
	<div class="col s12 m6 l4">
		<div class="card blue-grey darken-1">
			<div class="card-content white-text">
				<span class="card-title"><%= item.title %></span>
				<p><%= formatDate(item.inserted) %></p>
				<p>Ganastes §<%= item.value %></p>
			</div>
		</div>
	</div>
	<% }) %>

	<!-- BACK BUTTON -->
	<a class="btn-floating btn-large waves-effect waves-light floating-action" href="#!" onclick="apretaste.send({command:'ENCUESTA'})"><i class="material-icons">arrow_back</i></a>
</div>